<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.hncj.grabbing.mapper.OrderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.hncj.grabbing.pojo.Order">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="seats_id" property="seatsId" />
        <result column="seats_name" property="seatsName" />
        <result column="seats_count" property="seatsCount" />
        <result column="seats_price" property="seatsPrice" />
        <result column="order_channel" property="orderChannel" />
        <result column="status" property="status" />
        <result column="create_date" property="createDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, seats_id, seats_name, seats_count, seats_price, order_channel, status, create_date
    </sql>

</mapper>
